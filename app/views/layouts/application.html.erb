<% end %>

<%= yield %>

  <footer id="main_footer">
    <p class="copyright">
      &copy; 2017 Monika Piechatzek
    </p>
    <nav>
      <a href="https://twitter.com/piechatzek_" target="_blank"><i class="fa fa-twitter"></i></a>
      <a href="https://www.linkedin.com/in/monikapiechatzek" target="_blank"><i class="fa fa-linkedin"></i></a>
