  <%= csrf_meta_tags %>
  <title>Monika Piechatzek - Junior Software Developer Portfolio</title>
  <meta charset="utf-8">
  <link href='https://fonts.googleapis.com/css?family=Raleway:400,700' rel='stylesheet' type='text/css'>
